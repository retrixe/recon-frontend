{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react",
    "target": "es2016",
    "noImplicitAny": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "lib/",
    "pretty": true,
    "skipLibCheck": true,
    "allowJs": true
  },
  "include": [
    "pages",
    "components",
    "index.d.ts"
  ]
}